{
  "name": "inertia-drag",
  "version": "0.2.0",
  "author": "Yomotsu",
  "license": "MIT",
  "main": "dist/inertia-drag.js",
  "jsnext:main": "dist/inertia-drag.module.js",
  "module": "dist/inertia-drag.module.js",
  "types": "dist/index.d.ts",
  "repository": "yomotsu/inertia-drag",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.56.0",
    "eslint-config-mdcs": "^5.0.0",
    "rollup": "^4.9.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "terser": "^5.26.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "dev": "rollup --config --watch",
    "build": "rollup --config && terser dist/inertia-drag.js -o dist/inertia-drag.min.js --comments '/^!/'",
    "lint": "eslint src --ext ts"
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 11"
  ],
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "extends": "mdcs",
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "no-unused-vars": 0,
      "@typescript-eslint/no-unused-vars": 1,
      "indent": 0,
      "@typescript-eslint/indent": [
        "error",
        "tab",
        {
          "SwitchCase": 1,
          "flatTernaryExpressions": true
        }
      ],
      "no-multi-spaces": [
        0
      ],
      "key-spacing": [
        0
      ]
    }
  },
  "eslintIgnore": [
    "/dist/",
    "/node_modules/"
  ],
  "keywords": [
    "drag"
  ]
}
